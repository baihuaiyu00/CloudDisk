<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.log.mapper.FileOperationMapper">
    <insert id="addShareFile" parameterType="FileShareBean">
        insert into share(code,username,shareWebsite,fileName,realPath) values(#{code},#{userName},#{shareWebsite},#{fileName},#{realPath})
    </insert>
</mapper>

