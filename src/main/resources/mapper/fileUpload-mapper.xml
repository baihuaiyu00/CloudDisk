<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.log.mapper.FileOperationMapper">
    <!-- 添加个人办公机账号 -->
    <insert id="fileUpload" parameterType="FileUploadBean">
        insert into files(fileName,fileSize,updateTime,fileType,username,filePath) values(#{fileName},#{fileSize},#{updateTime},#{fileType},#{username},#{filePath})
    </insert>
</mapper>

